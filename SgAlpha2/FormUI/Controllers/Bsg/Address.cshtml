@inherits WebViewPage<AddressModel>

@using (var form = Html.ForModel(Model.Address))
{
    @form.Html.LabelledInputText(m => m.Line1).Control(c => c.Disabled(Model.Disabled))

    @form.Html.LabelledInputText(m => m.Line2).Control(c => c.Disabled(Model.Disabled))

    @form.Html.LabelledInputText(m => m.Line3).Control(c => c.Disabled(Model.Disabled))

    @form.Html.LabelledInputText(m => m.Postcode).Control(c => c.Disabled(Model.Disabled))
}
