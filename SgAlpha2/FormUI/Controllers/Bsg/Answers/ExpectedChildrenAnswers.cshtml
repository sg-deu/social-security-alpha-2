@inherits WebViewPage<ExpectedChildren>
<div class="answers-section">
    <h3>@BsgText.ExpectedChildrenTitle()</h3>
    <dl>
        @Html.AnswerFor(m => m.IsBabyExpected)
        @Html.AnswerFor(m => m.ExpectancyDate)
        @Html.AnswerFor(m => m.IsMoreThan1BabyExpected)
        @Html.AnswerFor(m => m.ExpectedBabyCount)
    </dl>
</div>
